package com.mission36.microservice.entity;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name = "student_tbl")
public class Student {

	private Long id;
	private String name;
	private Double jpa;

	public Student(Long id, String name, Double jpa) {
		super();
		this.id = id;
		this.name = name;
		this.jpa = jpa;
	}

}
